{
  "name": "electron-vite-template",
  "version": "0.0.1",
  "main": "dist-electron/electronMain.js",
  "author": "tt4g",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "npm-run-all -s clean:vite clean:electron vite:build electron:build",
    "dev": "npm-run-all -s vite:dev",
    "test": "npm-run-all -s vitest:run",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "electron:dev": "cross-env NODE_ENV=development electron",
    "electron:build": "electron-builder",
    "clean:vite": "rimraf ./dist ./dist-electron",
    "clean:electron": "rimraf ./dist-release",
    "vitest:run": "vitest",
    "vitest:coverage": "vitest run --coverage",
    "format": "eslint --fix ./src ./electron && prettier --write ./src ./electron",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./electron"
  },
  "devDependencies": {
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.24.3",
    "cross-env": "^7.0.3",
    "electron": "^21.2.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.1",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-electron": "^0.10.2",
    "vite-plugin-sass-dts": "^1.1.44",
    "vitest": "^0.24.3"
  },
  "dependencies": {
    "source-map-support": "^0.5.21"
  }
}
