{
  "name": "electron-vite-template",
  "version": "0.0.1",
  "main": "dist/electron/electronMain.js",
  "author": "tt4g",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "npm-run-all -s clean:vite clean:electron vite:build electron:build",
    "dev": "npm-run-all -s vite:dev",
    "test": "npm-run-all -s vitest:run",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "electron:dev": "cross-env NODE_ENV=development electron",
    "electron:build": "electron-builder",
    "clean:vite": "rimraf ./dist/src ./dist/electron",
    "clean:electron": "rimraf ./dist/release",
    "vitest:run": "vitest run",
    "vitest:snapshot": "vitest run --update",
    "vitest:watch": "vitest watch --update",
    "vitest:coverage": "vitest run --coverage --update",
    "format": "eslint --fix ./src ./electron && prettier --write ./src ./electron",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./electron"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.27.0",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-checker": "^0.5.3",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-sass-dts": "^1.2.12",
    "vitest": "^0.27.0",
    "vitest-mock-extended": "^1.0.8"
  },
  "dependencies": {
    "source-map-support": "^0.5.21"
  }
}
